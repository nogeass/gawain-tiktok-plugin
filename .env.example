# Gawain API Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# Base URL for Gawain API
GAWAIN_API_BASE=https://api.gawain.example.com

# API Key (optional for free preview; required for commercial usage)
# GAWAIN_API_KEY=your_api_key_here

# Application ID (if required)
GAWAIN_APP_ID=your_app_id_here

# Kinosuke upgrade URL for commercial usage
KINOSUKE_UPGRADE_URL=https://kinosuke.example.com/upgrade

# TikTok Shop API Configuration
TIKTOK_APP_KEY=your_tiktok_app_key
TIKTOK_APP_SECRET=your_tiktok_app_secret
# TIKTOK_ACCESS_TOKEN=your_access_token  # For CLI demo (from OAuth flow)
# TIKTOK_SHOP_CIPHER=your_shop_cipher    # Default shop for demo

# Polling configuration (optional)
# GAWAIN_POLL_INTERVAL_MS=2000
# GAWAIN_POLL_MAX_ATTEMPTS=60
